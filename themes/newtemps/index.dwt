<div class="indexmainBa">
  <div class="indexmax">
    <div class="main">
      <div class="Btn"></div>
      <a target="_blank" href="#"></a></div>
  </div>
  <div class="indexmin">
    <div class="main">
      <div class="Btn"></div>
      <a target="_blank" href="#"></a></div>
  </div>
</div>
<!-- #BeginLibraryItem "/library/page_header.lbi" --><!-- #EndLibraryItem -->
<div class="mfpSlide">
  <div class="slideBox main ">
    {insert name='getlist_ads' id=1 num=5}
  </div>
</div>
<div class="main  margin clearfix">
  <div class="indexLeft">
    <div class="indexLeftYY"></div>
    <div class="Panicbuying">{insert name='ads' id=2 num=1}</div>
    <div class="Announcement">
      <div class="AnnouncementTitle"><span class="cur">最新订单</span><span>公告</span></div>
      <div class="announcementC" style="display:block">
        <div class="TheyareBuyingC">
          <ul>
		  {foreach from=$order item=order}
            <li>
              <p class="lip1">{$order.user_name|truncate:8:"***":true}（{$order.usercity}）{$order.randtime}分钟前购买了{$order.goods_name}</p>
              <p class="lip2"><a target="_blank" href="{$order.goods_url}">{$order.goods_name}</a></p>
            </li>
 		  {/foreach}
          </ul>
        </div>
      </div>
      <div class="announcementC">
        <ul class="AnnouncementList">
		<!-- TemplateBeginEditable name="首页左侧公告文章" -->
<!-- #BeginLibraryItem "/library/cat_articles.lbi" -->

<!--{foreach from=$articles item=article_item}-->
<li><a href="{$article_item.url}" title="{$article_item.title|escape:html}">{$article_item.short_title}</a></li>
<!--{/foreach}-->
 <!-- #EndLibraryItem -->
<!-- TemplateEndEditable -->
        </ul>
      </div>
    </div>
  </div>
  <div class="indexRight">
    <div class="mod_day">
      <h4>活动推荐</h4>
    </div>
    <div class="slideTxtBox">
      {insert name='getlist_ads2' id=3 num=4}
    </div>
  </div>
</div>
<script type="text/javascript">
jQuery(".slideBox").slide( {mainCell:".bd ul",effect:"top",autoPlay:true,interTime:8000});
</script>
